<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/containterll"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/v_line_color_f6"

    android:orientation="vertical">


    <LinearLayout
        android:id="@+id/title_lin"
        android:layout_width="match_parent"
        android:layout_height="@dimen/title_bg_height"
        android:background="@color/white"
        android:orientation="vertical"></LinearLayout>


    <LinearLayout
        android:id="@+id/select_title_lin"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"></LinearLayout>


    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <LinearLayout
            android:id="@+id/main_buttom_lin"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:orientation="vertical">
        </LinearLayout>



        <LinearLayout
            android:id="@+id/main_lin"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_above="@id/main_buttom_lin"
            android:orientation="vertical"></LinearLayout>


    </RelativeLayout>


</LinearLayout>
