<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="15sp"
    android:paddingBottom="15sp"
    android:orientation="vertical" >
    
     <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:textColor="@color/black"
            android:text="315任意编码：" />
    
    
      <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="电阻：" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.su.usblink.UI.view.Re315RItemView
                android:id="@+id/re315_loc_r_item_1"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="wrap_content" />

        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="地址：" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.su.usblink.UI.view.Re315AddressItemView
                android:id="@+id/re315_loc_address_item_1"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315AddressItemView
                android:id="@+id/re315_loc_address_item_2"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315AddressItemView
                android:id="@+id/re315_loc_address_item_3"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315AddressItemView
                android:id="@+id/re315_loc_address_item_4"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <com.su.usblink.UI.view.Re315AddressItemView
            android:id="@+id/re315_loc_address_item_5"
            android:layout_width="match_parent"
             android:layout_weight="1"
            android:layout_height="wrap_content" />

        <com.su.usblink.UI.view.Re315AddressItemView
            android:id="@+id/re315_loc_address_item_6"
            android:layout_width="match_parent"
             android:layout_weight="1"
            android:layout_height="wrap_content" />

        <com.su.usblink.UI.view.Re315AddressItemView
            android:id="@+id/re315_loc_address_item_7"
            android:layout_width="match_parent"
             android:layout_weight="1"
            android:layout_height="wrap_content" />

        <com.su.usblink.UI.view.Re315AddressItemView
            android:id="@+id/re315_loc_address_item_8"
            android:layout_width="match_parent"
             android:layout_weight="1"
            android:layout_height="wrap_content" />
    </LinearLayout>
    
     <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical" >

        <TextView
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:text="数据：" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal" >

            <com.su.usblink.UI.view.Re315DataItemView
                android:id="@+id/re315_loc_data_item_1"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315DataItemView
                android:id="@+id/re315_loc_data_item_2"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315DataItemView
                android:id="@+id/re315_loc_data_item_3"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />

            <com.su.usblink.UI.view.Re315DataItemView
                android:id="@+id/re315_loc_data_item_4"
                android:layout_width="match_parent"
                 android:layout_weight="1"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

    

</LinearLayout>